{
  "name": "sc2-sdk",
  "version": "0.0.1",
  "description": "",
  "main": "lib/sc2.js",
  "scripts": {
    "build": "npm run build-browser && npm run build-node",
    "build-browser": "rm -rf dist && NODE_ENV=production webpack && gzip -k -f ./dist/*.js && du -h ./dist/*",
    "build-node": "mkdir -p ./lib && cp ./src/* ./lib/ && babel ./src --out-dir ./lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adcpm/sc2-sdk.git"
  },
  "keywords": [
    "steem",
    "steemit",
    "blockchain",
    "steemjs",
    "steemconnect"
  ],
  "author": "Bonustrack <fabien@bonustrack.co> (https://github.com/bonustrack)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/adcpm/sc2-sdk/issues"
  },
  "homepage": "https://github.com/adcpm/sc2-sdk#readme",
  "dependencies": {
    "debug": "^2.2.0",
    "isomorphic-fetch": "^2.2.1"
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-loader": "^6.2.5",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-es2017": "^6.16.0",
    "babel-register": "^6.14.0",
    "bluebird": "^3.4.6",
    "eslint": "^3.5.0",
    "eslint-config-airbnb": "^11.1.0",
    "eslint-plugin-import": "^1.15.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-react": "^6.2.1",
    "json-loader": "^0.5.4",
    "webpack": "^1.13.2",
    "webpack-visualizer-plugin": "^0.1.5"
  }
}
